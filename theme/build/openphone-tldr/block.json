{
  "apiVersion": 2,
  "name": "openphone/tldr",
  "title": "TLDR: Too Long Didnt Read",
  "icon": "megaphone",
  "category": "common",
  "editorScript": "file:./index.js",
  "attributes": {
    "title": {
      "type": "string",
      "source": "html",
      "selector": "h2"
    },
    "title2": {
      "type": "string",
      "source": "html",
      "selector": ".title2",
      "default": ""
    },
    "content": {
      "type": "string",
      "source": "html",
      "selector": ".content",
      "default": "Add your content here!"
    },
    "tldrText": {
      "type": "string",
      "source": "html",
      "selector": ".tldr-text",
      "default": "TL;DR"
    }
  },
  "style": {
    "title2": {
      "classes": "hidden sm:inline-block"
    }
  }
}